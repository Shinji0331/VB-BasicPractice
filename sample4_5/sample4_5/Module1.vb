Module Module1

    Sub Main()
        Dim X As Integer = 3
        Console.WriteLine("=====数値の比較=====")
        Console.WriteLine("X = 3 : {0}", X = 3)
        Console.WriteLine("X <> 3 : {0}", X <> 3)
        Console.WriteLine("X > 3 : {0}", X > 3)
        Console.WriteLine("X >= 3 : {0}", X >= 3)
        Console.WriteLine("X < 3 : {0}", X < 3)
        Console.WriteLine("X <= 3 : {0}", X <= 3)

        Console.ReadLine()

    End Sub

End Module
